    "url": "git+https://github.com/ineo6/mini-deploy.git"
  },
  "author": "ineo6",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ineo6/mini-deploy/issues"
  },
  "homepage": "https://github.com/ineo6/mini-deploy#readme",
  "dependencies": {
    "commander": "^3.0.0",
    "iconv-lite": "^0.4.24",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint-config-magic": "^1.1.1",
    "magic-lint": "^1.1.0"
  }
}
